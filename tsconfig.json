{
    "compilerOptions": {
        "target": "ES6",
        "allowJs": true,
        "module": "esnext",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "jsx": "react-jsx",
        "lib": ["ES2021", "DOM"],
        "baseUrl": ".",
        "outDir": "dist",
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "include": ["src/**/*"]
}
